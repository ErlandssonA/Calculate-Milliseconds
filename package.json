{
  "name": "calculate-milliseconds",
  "displayName": "Calculate Milliseconds",
  "description": "A Visual Studio Code extension to make it easier to work with milliseconds.",
  "author": "Erlandsson",
  "publisher": "Erlandsson",
  "version": "1.0.0",
  "license": "MIT",
  "icon": "icon.png",
  "pricing": "Free",
  "qna": "false",
  "galleryBanner": {
    "color": "#161616",
    "theme": "dark"
  },
  "keywords": [
    "timestamps",
    "milliseconds",
    "time",
    "calculate",
    "convert"
  ],
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErlandssonA/Calculate-Milliseconds"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.convertToReadable",
        "title": "Convert Milliseconds to Readable Time"
      },
      {
        "command": "extension.convertToMilliseconds",
        "title": "Convert Readable Time to Milliseconds"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test"
  }
}
